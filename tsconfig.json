{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "compilerOptions": {
    "noEmit": true,
    "strict": false,
    "sourceMap": true,
    "baseUrl": ".",
    "types": ["webpack-env"],
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": ["src/**/*", "registry/lib/**/*"],
  "exclude": ["dev", "packages", "typings"]
}
